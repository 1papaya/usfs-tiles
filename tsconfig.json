{
    "compilerOptions": {
      "baseUrl": "./",
      "target": "es5",
      "lib": ["dom", "dom.iterable", "esnext"],
      "allowJs": true,
      "skipLibCheck": true,
      "strict": false,
      "forceConsistentCasingInFileNames": true,
      "noEmit": true,
      "esModuleInterop": true,
      "moduleResolution": "node",
      "resolveJsonModule": true,
      "jsx": "preserve",
      "module": "commonjs",
      "isolatedModules": false,
    },
    "include": ["**/*.ts", "**/*.tsx"],
    "exclude": []
}
